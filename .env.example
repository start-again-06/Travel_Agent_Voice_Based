# Voice Travel Agent - Environment Variables Template
# Copy this file to .env and fill in your API keys
# Groq API (LLM) - Get from: https://console.groq.com
GROQ_API_KEY=gsk_your_key_here

# ElevenLabs API (Voice) - Get from: https://elevenlabs.io
ELEVENLABS_API_KEY=sk_your_key_here

# Resend API (Email) - Get from: https://resend.com
RESEND_API_KEY=re_your_key_here


# Pinecone (Vector DB for RAG) - Get from: https://pinecone.io
PINECONE_API_KEY=your_key_here

FOURSQUARE_SERVICE_API_KEY=your_key_here
FOURSQUARE_CLIENT_ID=your_id_here
FOURSQUARE_CLIENT_SECRET=your_secret_here

# Server configuration
SERVER_HOST=0.0.0.0
SERVER_PORT=8000

# Logging level (DEBUG, INFO, WARNING, ERROR)
LOG_LEVEL=INFO

# ElevenLabs Voice ID (default: Bella voice)
ELEVENLABS_VOICE_ID=EXAVITQu4vr4xnSDxMaL

# ElevenLabs Model ID (turbo for speed)
ELEVENLABS_MODEL_ID=eleven_turbo_v2_5


MAX_RECORDING_DURATION=60

WEBSOCKET_TIMEOUT=600


# Email sender (use default for Resend free tier)
EMAIL_SENDER=onboarding@resend.dev
EMAIL_SENDER_NAME=Voice Travel Agent

# Path to cache sentence-transformers models
SENTENCE_TRANSFORMERS_HOME=./models


# LangSmith tracing for debugging and monitoring LangChain/LangGraph agents
# Get API key from: https://smith.langchain.com
LANGSMITH_TRACING=true
LANGSMITH_ENDPOINT=https://api.smith.langchain.com
LANGSMITH_API_KEY=lsv2_pt_your_key_here
LANGSMITH_PROJECT=voice_agent
